{
  "name": "Kitstarter",
  "version": "2.0.0",
  "description": "Kitstarter Webpack",
  "author": "Matthieu Jacques",
  "scripts": {
    "dev-server": "webpack-dev-server --open --progress --colors --config ./config/webpack.config.js --env.dev-server",
    "start": "gulp delete-builds --gulpfile ./config/gulpfile.js && webpack --progress --colors --hide-modules --sort-assets-by size --config ./config/webpack.config.js --env.development",
    "stage": "gulp delete-builds --gulpfile ./config/gulpfile.js && webpack --progress --colors --hide-modules --sort-assets-by size --config ./config/webpack.config.js --env.staging",
    "build": "gulp delete-builds --gulpfile ./config/gulpfile.js && webpack --progress --colors --hide-modules --sort-assets-by size --config ./config/webpack.config.js --env.production",
    "jslint": "eslint ./src/js --color --config ./config/.eslintrc --ignore-path ./config/.lintignore || exit 0",
    "csslint": "stylelint ./src/scss/**/*.scss --color --config ./config/.stylelintrc --ignore-path ./config/.lintignore -f string -s scss || exit 0",
    "delete-builds": "gulp delete-builds --gulpfile ./config/gulpfile.js"
  },
  "keywords": [
    "webpack",
    "babel",
    "npm",
    "nodejs",
    "es2015",
    "eslint",
    "scss"
  ],
  "dependencies": {
    "bluebird": "3.5.0",
    "gsap": "1.19.1",
    "jquery": "3.2.1",
    "picturefill": "3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "7.0.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "bootstrap-sass": "^3.3.7",
    "chunk-manifest-webpack-plugin": "1.1.0",
    "copy-webpack-plugin": "4.0.1",
    "core-js": "2.4.1",
    "css-loader": "0.28.1",
    "del": "2.2.2",
    "eslint": "3.19.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.7.1",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "glob-all": "3.1.0",
    "gulp": "3.9.1",
    "html-webpack-plugin": "2.28.0",
    "mutationobserver-shim": "0.3.2",
    "node-sass": "4.5.2",
    "postcss-loader": "1.3.3",
    "sass-loader": "6.0.5",
    "style-loader": "0.17.0",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "stylelint-scss": "1.4.4",
    "stylelint-webpack-plugin": "0.7.0",
    "webpack": "2.5.1",
    "webpack-chunk-hash": "0.4.0",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-watch-time-plugin": "0.1.2"
  },
  "files": [
    "../README.md",
    "../CHANGELOG.md",
    "./config"
  ],
  "license": "UNLICENSED"
}
